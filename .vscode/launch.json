    "version": "0.2.0",
    "configurations": [
        {
            "name": "test-eterm",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/test/test-eterm",
            "args": ["-t", "test_map"],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            //"preLaunchTask": "C/C++: g++ build active file",
            "miDebuggerPath": "/usr/bin/gdb"
        },
        {
            "name": "test-node",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/src/test-node",
            "args": ["-n", "a@localhost", "-r", "iex-demo1@127.0.0.1", "-v", "trace"],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "lldb"
        }
    ]
}